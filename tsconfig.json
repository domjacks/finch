{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "jsx": "react",
    "rootDir": ".",
    "outDir": "build",
    "target": "es6",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "test/**/*.ts", "src/**/*.tsx"]
}
